{
  "manifest_version": 3,
  "name": "AI Tab Saver",
  "version": "1.0",
  "description": "Save your tab sessions smartly with AI",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "AI Tab Saver"
  }
}
