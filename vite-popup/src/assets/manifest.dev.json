{
  "manifest_version": 3,
  "name": "AI Tab Saver",
  "version": "1.0",
  "description": "Save and manage browser tabs",
  "action": {
    "default_popup": "popup.html"
    
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["http://localhost:3000"],
   "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
  
}
