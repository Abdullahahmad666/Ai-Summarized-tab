{
  "manifest_version": 3,
  "name": "AI Tab Saver",
  "version": "1.0",
  "description": "Save and manage your tabs smartly!",
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
    
  },
  
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
